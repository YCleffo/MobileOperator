<Page x:Class="MobileOperator.View.Pages.PersonalPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MobileOperator.View.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Личный кабинет">

    <Grid>
        <UniformGrid Columns="2">
            <Button x:Name="Expenses" Click="ExpensesClick">Расходы</Button>
            <Button x:Name="Setings" Click="SetingsClick">Настройка тарифа</Button>
            <Button x:Name="Services" Click="ServicesClick">Услуги</Button>
            <Button x:Name="Support" Click="SupportClick">Поддержка</Button>
        </UniformGrid>
    </Grid>
</Page>
